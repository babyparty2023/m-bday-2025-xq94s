:@template"E!/* Welcome to Compass.
 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */

@import "compass/reset";
@import "partials/settings";
$ph-width : 600px;
$ph-height: 300px;
$close-height:50%;
$color:#837362;

@mixin flip($tr_selector_before :"", $tr_selector_after: ":hover" , $tr_transform: rotateX(180deg), $direction :left top, $tr_duration :1s, $tr_delay:0s, $transition1:"", $tr_sel_duration :$tr_duration, $tr_sel_delay:$tr_delay, $transition1_sel:"") {
	& {	position: absolute;
		-webkit-transform-style: preserve-3d;
		-moz-transform-style: preserve-3d;
		@if ($transition1) !="" {
			-webkit-transition: -webkit-transform $tr_duration $tr_delay, #{$transition1};
			-moz-transition: -moz-transform $tr_duration $tr_delay, #{$transition1};
		}
		@else {
			-webkit-transition: -webkit-transform $tr_duration $tr_delay;
			-moz-transition: -moz-transform $tr_duration $tr_delay;
		}
		-webkit-transform-origin:$direction;
		-moz-transform-origin:$direction;
		}


	& > .front, & > .back  {position: absolute;
	width: 100%;
	height: 100%;
	-webkit-backface-visibility: hidden;
	-moz-backface-visibility: hidden;}
	 & > .back {-webkit-transform:$tr_transform;-moz-transform:$tr_transform}
	#{ $tr_selector_before } &#{ $tr_selector_after }  {-webkit-transform:$tr_transform;-moz-transform:$tr_transform; 
		@if (($tr_sel_duration) !="") or (($tr_sel_delay) !="") or (($transition1) !="")
			{
				@if ($transition1) !="" {
						-webkit-transition: -webkit-transform $tr_sel_duration $tr_sel_delay, #{$transition1_sel};
						-moz-transition: -moz-transform $tr_sel_duration $tr_sel_delay, #{$transition1_sel};
				}
				@else {
				@if ($tr_sel_duration) !="" {
					-webkit-transition-duration:  $tr_sel_duration ;
					-moz-transition-duration: $tr_sel_duration ;
				}
				@if ($tr_sel_delay) !="" 
					{-webkit-transition-delay:  $tr_sel_delay;
					-moz-transition-delay: $tr_sel_delay;}
				}

			}

		}

}

$link_color: #9b4849;
html {}
body {font-family:Satisfy, serif; color:$color;  background: #fbf8f7;}
h2 {font-size:1.5em; line-height:1.6;}

a {color:$link_color;text-decoration:none; display:block;}
a:hover {color:darken($link_color,10)};
#lid a {width:100%; text-align:center; margin-top:50px; }
#top a {padding:160px 20px 0 0;  }
.front a {margin-top:10px; }
#letter a {position:absolute; bottom:20px; right:40px;}



section.container {position:relative;
	width: $ph-width; height: $ph-height;
	margin: 280px auto 0;
	-webkit-perspective:1000px;-moz-perspective:1000px; 
   }
section.container > form.flip {width: 100%;
  height: 100%;
@include flip($tr_selector_before :"", $tr_selector_after: "" ,$tr_transform: rotateY(180deg), $direction :center center, $tr_duration :.7s); 
}

section.container:target > form.flip {-webkit-transform: rotateY(0deg);
-moz-transform: rotateY(0deg);}

.no-csstransforms3d section.container:target > form.flip > .back {display:none;}

#content { 
	background:$color;
  	box-shadow:inset 0 10px 30px -10px rgba(0,0,0,.3), inset 0 1px 0 rgba(0,0,0,.2), inset 0 2px 0   rgba(255,255,255,.2) ;	
	 
}
.container#lid {position:relative; width:100%; height:$close-height; -webkit-perspective:800px;-moz-perspective:800px;z-index:2;-moz-transition:z-index 0s 1s;-webkit-transition:z-index 0s 1s;}
.no-csstransforms3d .container#lid {-webkit-transition:none;}
#content:target .container#lid {z-index:-1; -moz-transition:z-index 0s .5s;-webkit-transition:z-index 0s .5s;}
.no-csstransforms3d #content:target .container#lid {-webkit-transition:none;}
.no-csstransforms3d .container#lid .back {display:none;}
.no-csstransforms3d #content:target .container#lid .back {display:block; top:-150px;}
#lid .flip {
	width: 100%;
	height: 100%;
	@include flip($tr_selector_before :"#content:target", $tr_selector_after: "" , $tr_transform: rotateX(180deg), $direction :left top, $tr_duration :.5s, $tr_delay:1s,  $tr_sel_delay:0s)
	
	 

}
 #letter .flip> div, #lid .flip > div, section.container > form.flip  > div {@include box-size; }

#lid .flip .front, section.container > form.flip .front {
	background: Beige url('../images/bg.png');	
	border:1px #eae1d5 solid;
	box-shadow: inset 0 0 10px 1px rgba(255,255,255,.6), 0 2px 3px -2px rgba(0,0,0,.6); 
	padding:20px 30px;  color:$color; text-shadow: 0 1px 0 #fff,0 1px 0 #fff;
	}
	section.container > form.flip > .front {font-size:1.25em; background: Beige url('../images/bg.png');	
		background: url('../images/stamp.png') no-repeat 450px 20px, url('../images/bg.png');
		
		
	
		padding:120px 20px 20px 40%;
		 border:1px #eae1d5 solid;
		}
#lid .flip .front {font-size:1.25em; border-radius: 0 0 40px 40px;}	
#lid .flip .back {background:$color; border-radius:  40px 40px 0 0;  border:35px solid rgba(255,255,255,.1); border-bottom:none; box-shadow: inset 0 10px 30px 10px rgba(0,0,0,.1);}


#top {text-align:right; background:url('../images/bg.png');
	background: -moz-linear-gradient(top, rgba(255,255,255,0) 0%, rgba(163,140,130,0.17) 100%), url('../images/bg.png');
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,0)), color-stop(100%,rgba(163,140,130,0.17))), url('../images/bg.png');
	background: -webkit-linear-gradient(top, rgba(255,255,255,0) 0%,rgba(163,140,130,0.17) 100%), url('../images/bg.png');
	background: -o-linear-gradient(top, rgba(255,255,255,0) 0%,rgba(163,140,130,0.17) 100%), url('../images/bg.png');
	background: -ms-linear-gradient(top, rgba(255,255,255,0) 0%,rgba(163,140,130,0.17) 100%), url('../images/bg.png');
	
	 
 @include box-size;  width:100%; height:65%; position:absolute; left:0; bottom:0;  z-index:1;border:1px #eae1d5 solid; box-shadow: inset 0 0 10px 1px rgba(255,255,255,.6), 0 -2px 4px -3px rgba(0,0,0,.75), 0 2px 3px -2px rgba(0,0,0,.6);}







#letter {background:#fafafa;  width:90%;  height:95%; position:absolute; left:5%; top:5%; z-index:0; 
-webkit-transition: .5s .5s;-moz-transition: .5s .5s;-o-transition: 0s 0s;}

#content:target #letter {top:-40%;-o-transition: .5s .5s;   }


#letter .container {position:absolute; width:100%; height:50%; -webkit-perspective:800px;  top:0; left:0; -webkit-transition: 0s 0s; 
	-moz-perspective:800px;   -moz-transition: 0s 0s; z-index:1;}
#letter .flip {
  width: 100%;
  height: 100%;
	@include flip($tr_selector_before :"#content:target", $tr_selector_after: "" , $tr_transform: rotateX(180deg), $direction :left top, $tr_duration :.5s, $tr_delay:0s, $tr_sel_delay:1s)

  
}
#content:target #letter .container {z-index:-1; -webkit-transition: z-index 0s 1.5s;-moz-transition: z-index 0s 1.5s; }
#letter .flip .front {background:#fafafa; box-shadow: 0 2px 3px 0 rgba(0,0,0,.2);}
.no-csstransforms3d #content:target #letter .flip .front {display:none;}
#letter .flip .back {background:#fafafa;background: -webkit-linear-gradient(top, rgba(250,250,250,1) 90%,rgba(247,247,247,1) 100%); 
	background:-moz-linear-gradient(top, rgba(250,250,250,1) 80%,rgba(247,247,247,1) 100%); 
	
	padding: 40px 40px 0; font-size:18px; line-height:1.4; border-bottom:1px solid rgba(255,255,255,.5); box-shadow: 0 -5px 8px -4px rgba(0,0,0,.15); }
.no-csstransforms3d #content:target #letter .flip .back {top:-140px;}

input[type="text"] {background:transparent; border:none; border-bottom:1px dotted $color; box-shadow:0 1px 0 rgba(255,255,255,.4); width: 50%;-webkit-appearance:none;}	
input[type="text"]:focus  {outline:none; border-bottom: 1px solid $color;-webkit-appearance:none;}
textarea {
	margin:10px auto 0;
width:90%;

	display:block;
	@include box-size;
padding: 10px ;
	background:transparent;
	background:rgba(255,255,255,.07);
	border:1px dashed lighten($color, 30);

	&:focus {outline:none;
		background-color:rgba(255,255,255,.12);
		border-color:rgba(255,255,255,.46);
		box-shadow:inset 0 0 2px rgba(0,0,0,.1);}
	}
textarea {
height:160px;
}
.textarea {position:relative;}
.textarea label {
	position:absolute;
	width:100%;
	text-align:center;

	-moz-transition:all 1s;-webkit-transition:all 1s;-o-transition:all 1s;transition:all 1s;}

input[type="submit"] {cursor:pointer; display:block; margin:10px auto;  -webkit-appearance:none; font-size:.9em; background: $link_color;padding:5px 20px; border-radius:3px; border:1px solid $link_color;
	box-shadow:inset 0 1px 0 rgba(255,255,255,.5); color:#fff;-webkit-transition:background .5s;}

input[type="submit"]:hover {background:darken($link_color,10);}:
